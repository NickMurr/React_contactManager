{
  "extends": ["react-app", "prettier", "airbnb"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": ["error", { "singleQuote": true }],
    "as-needed": { "requireForBlockBody": true }
  },
  "plugins": ["prettier", "react", "jsx-a11y", "import"]
}
